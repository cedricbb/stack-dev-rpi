[mysqld]
innodb_buffer_pool_size = 256M
innodb_log_file_size = 64M
innodb_flush_method = O_DIRECT
innodb_flush_log_at_trx_commit = 2
max_connections = 100
key_buffer_size = 32M
table_open_cache = 400
sort_buffer_size = 4M
read_buffer_size = 2M
read_rnd_buffer_size = 4M
query_cache_size = 64M
max_allowed_packet = 16